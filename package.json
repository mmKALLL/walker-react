{
  "name": "walker-react",
  "version": "1.0.0",
  "private": true,
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "globals": {
      "walker": true
    },
    "rules": {
      "semi": "off",
      "padded-blocks": "off"
    }
  },
  "scripts": {
    "watch": "yarn sass & yarn babel",
    "sass": "sass --watch styles:build/styles",
    "babel": "babel --watch src/** --out-file build/walker.js --presets react-app/prod --source-maps"
  },
  "dependencies": {
    "@types/node": "^11.13.8",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "babel-cli": "^7.0.0",
    "babel-preset-react-app": "3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
