{
  "name": "walker-react",
  "version": "1.0.0",
  "private": true,
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "globals": {
      "walker": true
    },
    "rules": {
      "semi": "never",
      "padded-blocks": "off"
    }
  },
  "scripts": {
    "watch": "yarn sass & yarn babel",
    "sass": "sass --watch styles:build/styles",
    "babel": "babel --watch src --out-file walker.js --source-maps",
    "check-types": "tsc --jsx react"
  },
  "dependencies": {
    "@types/node": "^11.13.8",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.4",
    "babel-cli": "7.0.0-beta.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "7.4.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "typescript": "^3.4.5"
  }
}
